{
  "name": "beavis-and-butthead-bot",
  "version": "0.1.0",
  "description": "Heh heh heh. A Slackbot that doesn't suck. These two juvenile clowns respond to your messages with childish comments.",
  "engines": {
    "node": "~4.4"
  },
  "keywords": [
    "slack",
    "slackbot",
    "bot",
    "beavis",
    "butthead"
  ],
  "main": "./lib/server.js",
  "scripts": {
    "clean": "rm -fr ./lib/* && rm -fr ./build/*",
    "build": "npm run clean && npm run cpsrc2lib && NODE_ENV=production babel lib --out-dir lib",
    "build:dev": "npm run clean && npm run cpsrc2lib && NODE_ENV=development babel lib --out-dir lib",
    "start": "NODE_ENV=production node ./lib/server.js",
    "start:dev": "npm run build:dev && NODE_ENV=development SLACK_TOKEN=xoxb-58351512401-NYqRA2mb4FW39E7GqdzghRxH node ./lib/server.js",
    "predeploy": "rsync -av * build --del --exclude=\"build\" --exclude=\"src\" --exclude=\"test\" --exclude=\"interfaces\" --exclude=\"node_modules\"",
    "deploy": "gh-pages -b master -d build",
    "cpsrc2lib": "cp -r src/* lib",
    "lint": "eslint src",
    "test": "echo Write some tests, dammit!"
  },
  "author": {
    "name": "Donavon West",
    "email": "email@donavon.com",
    "url": "http://donavon.com"
  },
  "license": "MIT",
  "dependencies": {
    "beepboop-botkit": "^1.4.1",
    "bluebird": "^3.4.2",
    "botkit": "^0.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.0.1",
    "gh-pages": "^0.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/donavon/beavis-and-butthead-bot.git"
  },
  "bugs": {
    "url": "https://github.com/donavon/beavis-and-butthead-bot/issues"
  },
  "homepage": "https://github.com/donavon/beavis-and-butthead-bot"
}
